# OpenAI API key for accessing OpenAI's services
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API key for accessing Claude models
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Set to True for testing mode, False for production
TEST_MODE=False

# LangSmith configuration for tracing and monitoring
LANGSMITH_TRACING_V2=true
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY="your_langsmith_api_key_here"
LANGSMITH_PROJECT="your_project_name"

# Qdrant vector database configuration
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_URL=your_qdrant_url_here

# ALP (Automated Learning Platform) credentials
ALP_EMAIL=your_email_here
ALP_PASSWORD=your_password_here

# S3 bucket base URL for storing and accessing files
S3_BASE_URL=your_s3_base_url_here